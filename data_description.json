{
    "title": "Data Description of 01_data/analysis/data_analysis.csv",
    "description": "This dataset contains the data used for all reported analyses (Cognitive Control & Motivated Reasoning)",
    "columns": {
        "subj_idx": {
            "Description": "Unique subject identifier for each participant",
            "Units": "integer"
        },
        "age_corrected": {
            "Description": "Age of the participant, corrected based on communication with participants on Prolific in some cases",
            "Units": "years"
        },
        "age_group": {
            "Description": "Categorical age group of the participant",
            "Levels": {
                "18-24": "Participants aged 18 to 24",
                "25-31": "Participants aged 25 to 31",
                "Above 32": "Participants aged 32 and above"
            }
        },
        "gender": {
